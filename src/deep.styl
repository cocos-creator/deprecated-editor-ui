html /deep/ {
    @import "common"

    fire-ui-tab.active + fire-ui-tab /deep/ span.left-tab-border {
        display: inline-block
        border-left: 1px solid #212121
        height: 20px
    }

    .fa {
        display: inline-block;
        font-family: FontAwesome;
        font-style: normal;
        font-weight: normal;
        line-height: 1;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .fa-check:before {
        content: "\f00c";
    }

    .fa-caret-down:before {
        content: "\f0d7";
    }

    .fa-caret-up:before {
        content: "\f0d8";
    }

