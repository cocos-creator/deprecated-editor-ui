.flex-row {
    display: flex
    flex-direction: row
    flex-wrap: nowrap     
    align-items: center
}

.flex-col {
    display: flex
    flex-direction: column
    flex-wrap: nowrap     
    align-items: center
}

.flex-stretch {
    align-items: stretch
}

.flex-self-start {
    align-self: flex-start
}

.flex-1 {
    flex: 1 1 60px
}

